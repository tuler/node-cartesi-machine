{
    "name": "@tuler/node-cartesi-machine",
    "version": "0.5.0",
    "description": "Cartesi Machine Node.js bindings",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "require": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "build": "tsup",
        "docs:dev": "vocs dev",
        "docs:build": "vocs build",
        "docs:preview": "vocs preview",
        "lint": "biome check",
        "lint:ci": "biome ci",
        "release": "tsup && changeset publish",
        "test": "vitest"
    },
    "keywords": [
        "cartesi",
        "bindings",
        "node.js"
    ],
    "author": "Danilo Tuler",
    "license": "Apache-2.0",
    "dependencies": {
        "koffi": "^2.12.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.0.6",
        "@changesets/cli": "^2.29.5",
        "@types/node": "^24.0.10",
        "npm-run-all": "^4.1.5",
        "tsup": "^8.5.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "viem": "^2.31.7",
        "vitest": "^3.2.4",
        "vocs": "^1.0.12"
    },
    "files": [
        "dist"
    ],
    "packageManager": "pnpm@10.12.4"
}
